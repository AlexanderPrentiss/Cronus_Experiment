    "def start_clients(HOST, PORT, num_models, learning_rounds, local_epochs, collab_epochs):\n",
    "\n",
    "    sampleSize = len(X_train)//num_models\n",
    "    imgSize = len(X_train[0])\n",
    "    X_trains = torch.zeros((sampleSize, imgSize, num_models))\n",
    "    Y_trains = torch.zeros((sampleSize, num_models))\n",
    "    loss_fn = nn.CrossEntropyLoss()\n",
    "\n",
    "    # Fill data for each model\n",
    "    for m in range(num_models):\n",
    "        # Random indices for this model\n",
    "        idx = torch.randperm(len(X_train))[:sampleSize]\n",
    "\n",
    "        # Assign data\n",
    "        X_trains[:, :, m] = X_train[idx]\n",
    "        Y_trains[:, m]    = Y_train[idx]\n",
    "\n",
    "    models = []\n",
    "\n",
    "    s = socket.socket(socket.AF_INET, socket.SOCK_STREAM)\n",
    "    s.connect((HOST, PORT))\n",
    "    print(f\"Connected to {HOST} on port {PORT}\")\n",
    "\n",
    "    # RECEIVE PUBLIC FEATURES\n",
    "    features = recv_tensor(s)\n",
    "    print(features.shape)\n",
    "\n",
    "    # Create models\n",
    "    models = [MnistModel() for _ in range(num_models)]\n",
    "\n",
    "    for r in range(learning_rounds):\n",
    "\n",
    "        print(f\"Learning round {r}\")\n",
    "\n",
    "        # Loop to train each model locally\n",
    "        for i in range(num_models):\n",
    "            optimSGD = SGD(params=models[i].parameters(), lr=0.01)\n",
    "            XTR = X_trains[:,:,i]\n",
    "            YTR = Y_trains[:,i].long()\n",
    "            print(f\"Fitting model {i}...\")\n",
    "            models[i].fit(XTR, YTR, optimSGD, loss_fn, local_epochs)\n",
    "\n",
    "        # Do predictions on all of the public dataset\n",
    "        predictions = []\n",
    "        for i in range(num_models):\n",
    "            prediction = models[i].forward(features).detach()\n",
    "            predictions.append(prediction)\n",
    "        predictions = torch.stack(predictions, axis=0)\n",
    "\n",
    "        # SEND LABEL PREDICTIONS\n",
    "        send_tensor(s, predictions)\n",
    "\n",
    "        # RECEIVE AGGREGATED LABELS\n",
    "        aggregationLabels = recv_tensor(s)\n",
    "\n",
    "        for i in range(num_models):\n",
    "            optimAdam = Adam(params=models[i].parameters(), lr=0.0001)\n",
    "            models[i].fit(features, aggregationLabels, optimAdam, loss_fn, collab_epochs)\n",
    "\n",
    "        # Get error rates\n",
    "        for i in range(num_models):\n",
    "            preds = models[i].forward(X_test).argmax(dim=1)\n",
    "            errorRate = (preds != Y_test).float().mean()\n",
    "            print(f\"\\tModel {i} Error: {errorRate}\")\n",
    "\n",
    "    s.close()\n",
    "\n",
